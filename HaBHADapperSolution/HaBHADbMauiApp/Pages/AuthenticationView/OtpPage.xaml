<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HaBHADbMauiApp.Pages.AuthenticationView.OtpPage"
             Title="OtpPage">
    
    <VerticalStackLayout>
        <Entry x:Name="TxtOtp" Placeholder="Enter Code" TextColor="Black" HeightRequest="40" Background="White"/>
        <Button x:Name="BtnSubmit" Clicked="BtnSubmit_Clicked" Text="Submit" WidthRequest="150"/>
    </VerticalStackLayout>
</ContentPage>